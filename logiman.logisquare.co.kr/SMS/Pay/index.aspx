<%@ Page Language="C#" MasterPageFile="~/Sms.Master"  AutoEventWireup="true" CodeBehind="Index.aspx.cs" Inherits="SMS.Pay.Index" %>

<asp:Content ID="Scriptcontent" ContentPlaceHolderID="headscript" Runat="Server">
    <script src="/SMS/Pay/Proc/Index.js?var=<%=DateTime.Now.ToString("yyyyMMddHHmmss")%>"></script>
    <script src="/SMS/js/CargoManagerBanner.js?ver=<%=DateTime.Now.ToString("yyyyMMddHHmmss")%>"></script>
    <script type="text/javascript">
        $(document).ready(function () {

            $("#pwd1").focus();

            $("#pwd1").on("keyup",
                function () {
                    if ($(this).val().length === 1) {
                        $("#pwd2").focus();
                    }
                });

            $("#pwd2").on("keyup",
                function (event) {
                    if (event.keyCode == 8) {
                        $("#pwd1").focus();
                    } else {
                        if ($(this).val().length === 1) {
                            $("#BtnChkCorpNo").click();
                        }
                    }
                });

            //사업자번호 확인
            $("#BtnChkCorpNo").on("click", function() {
                fnChkCorpNo();
            });
        });

        function fnOpenEvent(url) {
            //fnBannerEventHistRequest(2, "CargopayMembershipPopup", "카고페이멤버십(MMP041)");
            document.location.replace(url);
        }

        function fnEventClose() {
            $("div.event_popup").toggle();
            $("#pwd1").focus();
        }
    </script>
</asp:Content>

<asp:Content ID="BodyContent" ContentPlaceHolderID="MainContent" runat="server">
    <asp:HiddenField runat="server" ID="DisplayMode"/>
    <asp:HiddenField runat="server" ID="ErrMsg"/>
    <asp:HiddenField runat="server" ID="No"/>
    <asp:HiddenField runat="server" ID="HidPageTitle" Value="입금 예정 안내(MMP041)"/>
    <asp:HiddenField runat="server" ID="HidUserName" />
    <asp:HiddenField runat="server" ID="HidUserCell" />
    <asp:HiddenField runat="server" ID="HidUserNo" />
    <asp:HiddenField runat="server" ID="HidEventViewFlag" />
    <asp:HiddenField runat="server" ID="HidEventAvailChkFlag" />
    <div id="wrap">
    
        <!-- 사업자번호 인증 -->
        <div class="opacity_bg">
            <div class="self_area">
                <h3>
                    본인 확인을 위해 사업자 번호​<br>뒤 두 자리를 입력해 주세요​.
                </h3>
                <dl>
                    <dt>사업자번호</dt>
                    <dd>
                        <p>
                            <span id = "CorpNo"></span>
                            <input type="password" runat="server" id="pwd1" maxlength="1" placeholder="*" inputmode="numeric" />
                            <input type="password" runat="server" id="pwd2" maxlength="1" placeholder="*" inputmode="numeric" />
                        </p>
                    </dd>
                    <dd class="button">
                        <button type="button" id="BtnChkCorpNo">확인</button>
                    </dd>
                </dl>
                <p>
                    본 서비스는 운송사가 아닌​<br>카고페이에서 제공합니다.​<br><br>(고객센터 1522-9766)
                </p>
                <div class="self_logo">
                    <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="81" height="19" viewBox="0 0 81 19">
                        <defs>
                          <pattern id="pattern" preserveAspectRatio="xMidYMid slice" width="100%" height="100%" viewBox="0 0 471 111">
                            <image width="471" height="111" xlink:href="data:image/png;base64,"/>
                          </pattern>
                        </defs>
                        <rect id="pay-h-light_3x" data-name="pay-h-light@3x" width="81" height="19" fill="url(#pattern)"/>
                    </svg>
                </div>
            </div>
        </div>
    </div>
    <!--이벤트팝업 레이어 시작-->
    <div class="event_popup">
        <table border="0" cellspacing="0">
            <tr>
                <td><a href="javascript:fnEventClose();"><img src="/SMS/images/event/event_popup_20240715.png" class="link_img" border="0"/></a></td>
            </tr>
            <tr>
                <td style="text-align:center; padding-top:10px;">
                    <a href="javascript:fnEventClose();"><img src="/SMS/images/close_btn_w.png" alt="Alternate Text" style="width:50px; border:4px solid #fff; border-radius:100px;"/></a>
                </td>
            </tr>
        </table>
    </div>
    <!--이벤트팝업 레이어 끝-->
</asp:Content>